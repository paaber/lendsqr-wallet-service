{
  "name": "LendsQR-Wallet",
  "version": "0.0.0",
  "scripts": {
    "build": "npx ts-node build.ts",
    "lint": "npx eslint --ext .ts src/",
    "lint:tests": "npx eslint --ext .ts spec/",
    "start": "node -r module-alias/register ./dist/src/index.js --env=production",
    "dev": "nodemon",
    "debug": "nodemon --exec \"node --inspect-brk=0.0.0.0:9229 --require ts-node/register src/server.ts\"",
    "test": "nodemon --config ./spec/nodemon.json",
    "test:no-reloading": "npx ts-node --files -r tsconfig-paths/register ./spec",
    "format": "npx prettier --write 'src/**'",
    "windows-prisma:migrate": "dotenv -e %ENV_FILE% -- npx prisma migrate dev",
    "prisma:studio": "dotenv -e %ENV_FILE% -- npx prisma studio"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "./node_modules/.bin/ts-node --files -r tsconfig-paths/register ./src ",
    "execMap": {
      "ts": "node --require ts-node/register/transpile-only"
    }
  },
  "_moduleAliases": {
    "@root": "dist",
    "@src": "dist/src"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/uuid": "^9.0.8",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.5.0",
    "helmet": "^7.1.0",
    "inserturlparams": "^1.0.1",
    "jet-logger": "^2.0.1",
    "knex": "^3.1.0",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "mysql2": "^3.12.0",
    "swagger-ui-express": "^5.0.0",
    "ts-command-line-args": "^2.5.1",
    "uuid": "^9.0.1",
    "validator": "^13.11.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/find": "^0.2.4",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.7",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/validator": "^13.11.8",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "eslint": "^8.56.0",
    "eslint-plugin-node": "^11.1.0",
    "find": "^0.3.0",
    "fs-extra": "^11.2.0",
    "jasmine": "^5.1.0",
    "nodemon": "^3.1.4",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
